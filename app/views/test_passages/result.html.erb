<h1><%= t('.header', title: @test_passage.test.title)%></h1>
<h1><%= t('.correct_count', correct: @test_passage.correct_questions) %></h1>
 <% if @test_passage.test_passed_successfully?%>
    <p style= "color:green"><%= t('.passed', score: @test_passage.percentage_score )%></p>
 <%else%>
   <p style= "color:red"><%= t('.not_passed', score: @test_passage.percentage_score )%></p>
 <%end%>

<p><%= link_to t('nav.all_tests'), tests_path%></p>
