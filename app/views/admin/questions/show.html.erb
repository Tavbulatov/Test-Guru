<h1><%= t('.header')%></h1>

<p><%= link_to t('nav.new_answer'), new_admin_question_answer_path(@question), class: 'btn btn-light'%></p>

<table class="table table-striped">
  <%= render @question.answers%>
</table>


<p>
  <%= link_to t('nav.back'), admin_test_path(@question.test), class: 'btn btn-light'%>
</p>
